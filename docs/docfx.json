{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "Joueur.cs.csproj"
          ]
        }
      ],
      "dest": "games",
      "disableGitFeatures": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "games/**.yml",
          "games/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "overwrite": [
      {
        "exclude": [
          "output/**"
        ]
      }
    ],
    "dest": "output",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_disableContribution": true,
      "_appTitle": "C# Joueur Client"
    },
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
